\examples{
if(require("INLA",quietly=TRUE)){
  library(longmemo)
  n <- 300
  H <- 0.8
  m <- 4
  forcing <- arima.sim(n=n, list(ar=c(0.6)),sd = 1)
  fgnsim <- simFGN0(n,H)

  fgnmean <- numeric(n)
  struktur <- (0.5+seq(0,n-1,length.out=n))^(0.8-3/2)
  for(i in 1:n){
      fgnmean[i] <- rev(struktur[1:i])\%*\%forcing[1:i]
  }
  y <- fgnsim + fgnmean

  result.climate <- inla.climate(data=y,forcing=forcing,m=m,Qco2=NULL)
  Qco2 <- 3.8
  result.mu <- inla.climate.mu(result.climate$inla.result,forcing,nsamples=10000)
  }
}
